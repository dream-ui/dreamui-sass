{
  "name": "dreamui-sass",
  "version": "1.0.0",
  "description": "DreamUI sass lib. contain some of common mixin and style definition.",
  "main": "index.scss",
  "scripts": {
    "lint": "node_modules/.bin/sass-lint",
    "sass": "sass --sourcemap=none test/test.scss test/test.css && sass --sourcemap=none test/flex.scss test/flex.css",
    "test:watch": "sass --sourcemap=none --watch test/:test/",
    "test:liveload": "node_modules/.bin/browser-sync start --server 'test' --files 'test/*.css, test/*.html'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borenXue/tools_sass.git"
  },
  "keywords": [
    "sass",
    "tool"
  ],
  "author": "boren.xue",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dream-ui/dreamui-sass/issues"
  },
  "homepage": "https://github.com/dream-ui/dreamui-sass/#readme",
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "sass-lint": "^1.10.2"
  }
}
